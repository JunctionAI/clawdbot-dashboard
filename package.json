{
  "name": "clawdbot-saas-dashboard",
  "version": "2.0.0",
  "private": true,
  "description": "Premium SaaS dashboard for Clawdbot - AI assistant platform",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "clean": "rm -rf .next out node_modules/.cache",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:unit": "jest --testPathPattern=__tests__/unit",
    "test:integration": "jest --testPathPattern=__tests__/integration",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui"
  },
  "dependencies": {
    "@stripe/stripe-js": "^2.4.0",
    "axios": "^1.6.5",
    "framer-motion": "^11.18.2",
    "lucide-react": "^0.462.0",
    "next": "^14.2.22",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "stripe": "^14.14.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.41.0",
    "@testing-library/jest-dom": "^6.4.0",
    "@testing-library/react": "^14.2.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8",
    "prettier": "^3.2.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "saas",
    "dashboard",
    "ai",
    "assistant",
    "premium-ui",
    "nextjs",
    "react",
    "typescript"
  ],
  "author": "Clawdbot Team",
  "license": "UNLICENSED"
}
